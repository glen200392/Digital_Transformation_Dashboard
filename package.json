{
  "name": "digital-transformation-dashboard",
  "version": "2.2.0",
  "description": "A comprehensive, real-time dashboard for tracking and visualizing digital transformation initiatives with 144% ROI and 90% efficiency gain",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server -p 8080 -o",
    "dev": "npx http-server -p 8080 -c-1",
    "serve": "python3 -m http.server 8080",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,css,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,css,html,md}\"",
    "test": "echo \"Tests not yet implemented\" && exit 0",
    "validate": "npm run lint && npm run format:check",
    "docs": "echo \"Opening documentation...\" && open DOCUMENTATION_INDEX.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glen200392/Digital_Transformation_Dashboard.git"
  },
  "keywords": [
    "dashboard",
    "digital-transformation",
    "kpi",
    "visualization",
    "chart.js",
    "google-sheets",
    "real-time",
    "analytics",
    "business-intelligence",
    "enterprise"
  ],
  "author": "Digital Transformation Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glen200392/Digital_Transformation_Dashboard/issues"
  },
  "homepage": "https://github.com/glen200392/Digital_Transformation_Dashboard#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "http-server": "^14.1.1",
    "prettier": "^3.1.1"
  },
  "optionalDependencies": {
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1",
    "frappe-gantt": "^0.6.1",
    "dayjs": "^1.11.10",
    "gridstack": "^9.0.0"
  },
  "peerDependencies": {},
  "browserslist": [
    "Chrome >= 90",
    "Firefox >= 88",
    "Safari >= 14",
    "Edge >= 90"
  ],
  "config": {
    "port": "8080"
  },
  "private": false
}
